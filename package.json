{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode admin",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "dev:responsavel": "vite --mode responsavel",
    "dev:aluno": "vite --mode aluno",
    "dev:admin": "vite --mode admin",
    "dev:all": "concurrently -n resp,aluno,admin -c green,blue,yellow \"npm run dev:responsavel\" \"npm run dev:aluno\" \"npm run dev:admin\"",
    "build:responsavel": "vite build --mode responsavel",
    "build:aluno": "vite build --mode aluno",
    "build:admin": "vite build --mode admin",
    "build:all": "npm run build:responsavel && npm run build:aluno && npm run build:admin"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.10.0",
    "bootstrap": "^5.3.7",
    "jquery": "^3.7.1",
    "pinia": "^3.0.3",
    "sweetalert2": "^11.22.2",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue-tippy": "^6.7.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/bootstrap": "^5.2.10",
    "@types/jquery": "^3.5.32",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-oxlint": "~1.1.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.1.0",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10"
  }
}
